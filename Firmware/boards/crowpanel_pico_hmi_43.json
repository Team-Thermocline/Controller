{
  "build": {
    "core": "rp2040",
    "cpu": "cortex-m0plus",
    "extra_flags": "-DPICO_DEFAULT_UART=0 -DPICO_DEFAULT_UART_TX_PIN=0 -DPICO_DEFAULT_UART_RX_PIN=1 -DARDUINO_ARCH_RP2040",
    "f_cpu": "133000000L",
    "f_flash": "20000000L",
    "flash_mode": "dio",
    "ldscript": "memmap_default.ld",
    "mcu": "rp2040",
    "variant": "rp2040"
  },
  "debug": {
    "onboard_tools": [
      "cmsis-dap"
    ],
    "openocd_board": "raspberrypi-swd",
    "openocd_target": "rp2040",
    "svd_path": "RP2040.svd"
  },
  "frameworks": [
    "arduino",
    "pico-sdk"
  ],
  "name": "CrowPanel Pico HMI 4.3-inch",
  "platform": "https://github.com/maxgerhardt/platform-raspberrypi.git",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 2097152,
    "protocol": "picotool",
    "protocols": [
      "picotool",
      "cmsis-dap"
    ]
  },
  "url": "https://www.elecrow.com/wiki/CrowPanel_Pico_HMI_Display-4.3.html",
  "vendor": "Elecrow",
  "version": "1.0.0"
}
